{
  "name": "@luciancaetano/lens-ui-forms",
  "version": "1.0.0",
  "main": "build/main.min.js",
  "files": [
    "build"
  ],
  "types": "build/types/index.d.ts",
  "description": "Lucian's Design System",
  "scripts": {
    "build": "rimraf build/ && webpack --mode production && npm run check:licenses && rimraf types/ && npm run types",
    "test": "jest --coverage --silent",
    "check:licenses": "license-checker --production --csv --out build/licenses.csv",
    "test:dev": "jest --watchAll --coverage",
    "storybook": "start-storybook -p 3210",
    "storybook:export": "npm run build && build-storybook",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:fix": "npm run lint:code:fix && npm run lint:styles --fix ",
    "types": "tsc src/index.ts --declaration --allowJs --emitDeclarationOnly --outDir build/types --esModuleInterop --jsx react --skipLibCheck",
    "lint:code": "eslint ./src --ext .tsx --ext .ts --max-warnings=0 && eslint ./stories --ext .tsx --ext .ts --max-warnings=0",
    "lint:code:fix": "eslint ./src --ext .tsx --ext .ts --fix && eslint ./stories --ext .tsx --ext .ts --fix",
    "lint:styles": "stylelint \"src/**/*.{ts,tsx,scss,css}\"",
    "release:minor": "standard-version --release-as minor && npm run build && git push --follow-tags origin master && npm publish",
    "release:major": "standard-version --release-as major && npm run build && git push --follow-tags origin master && npm publish",
    "release:patch": "standard-version --release-as patch && npm run build && git push --follow-tags origin master && npm publish",
    "release:pre": "standard-version --prerelease && npm run build && git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancaetano/lens-ui.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Typescript",
    "Design System",
    "UX",
    "UI",
    "Storybook"
  ],
  "author": "Lucian Caetano",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/luciancaetano/lens-ui/issues"
  },
  "homepage": "https://github.com/luciancaetano/lens-ui#readme",
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-typescript": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@namics/stylelint-bem": "^6.3.4",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-viewport": "^6.3.7",
    "@storybook/builder-webpack5": "^6.3.7",
    "@storybook/manager-webpack5": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.3.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "jest-useragent-mock": "^0.1.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.1.2",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^2.2.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.38.2",
    "sass-loader": "^12.1.0",
    "standard-version": "^9.3.1",
    "storybook-mobile": "^0.1.32",
    "style-loader": "^3.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.20.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint",
      "jest --bail --findRelatedTests",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss"
    ],
    "src/**/*.css": [
      "stylelint"
    ],
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "gacp": {
    "push": true,
    "emoji": "code"
  },
  "dependencies": {
    "@luciancaetano/lens-ui": "^2.0.0",
    "clsx": "^1.1.1",
    "react-hook-form": "^7.14.2"
  }
}
